{
  "namespace": "org.kaaproject.ipc.event.gen.v1.endpoint.config",
  "type": "record",
  "name": "EndpointMetadataUpdatedEvent",
  "doc": "The endpoint metadata updated event message.",
  "fields": [
    {
      "name": "correlationId",
      "type": "string",
      "default": "",
      "doc": "Message tracing ID primarily used for tracking the message processing across services."
    },
    {
      "name": "timestamp",
      "type": "long",
      "doc": "Unix time in milliseconds when the message was created."
    },
    {
      "name": "timeout",
      "type": "long",
      "default": -1,
      "doc": "The amount of milliseconds since timestamp when the message expires at the originating entity."
    },
    {
      "name": "endpointId",
      "type": "string",
      "doc": "The endpoint identifier."
    },
    {
      "name": "appVersionName",
      "type": "string",
      "default": "",
      "doc": "The application version name the endpoint registered in."
    },
    {
      "name":"removed",
      "type":{
        "type": "map",
        "values":{
          "type": "record",
          "namespace": "org.kaaproject.events.endpoint.lifecycle.gen.v1",
          "name": "MetadataKeyRemovedDto",
          "fields": [
            {
              "name": "oldValue",
              "type": ["null", "string"],
              "default": null
            }
          ]
        }
      },
      "doc": "The map of metadata keys to MetadataKeyRemovedDto with JSON string of old values that was removed."
    },
    {
      "name": "added",
      "type": {
        "type": "map",
        "values":{
          "type": "record",
          "namespace": "org.kaaproject.events.endpoint.lifecycle.gen.v1",
          "name": "MetadataKeyAddedDto",
          "fields": [
            {
              "name": "newValue",
              "type": ["null", "string"],
              "default": null
            }
          ]
        }
      },
      "doc": "The map of metadata keys to the MetadataKeyAddedDto with JSON string of old new values that was added."
    },
    {
      "name": "updated",
      "type": {
        "type": "map",
        "values":{
          "type": "record",
          "namespace": "org.kaaproject.events.endpoint.lifecycle.gen.v1",
          "name": "MetadataKeyUpdatedDto",
          "fields": [
            {
              "name": "newValue",
              "type": ["null", "string"],
              "default": null
            },
            {
              "name": "oldValue",
              "type": ["null", "string"],
              "default": null
            }
          ]
        }
      },
      "doc": "The map of metadata keys to MetadataKeyUpdatedDto with JSON Strings of old and new values that was updated."
    },
    {
      "name": "originatorReplicaId",
      "type": [
        "null",
        "string"
      ],
      "default": null,
      "doc": "Identifier of the service replica that generated the event."
    }
  ]
}
